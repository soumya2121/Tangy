@model IEnumerable<Tangy.Models.SubCategory>
@{
    ViewData["Title"] = "Index";
}

<br />
<h2>Index</h2>
<br />
@Html.Partial("_CreateButtonPartial")
<div class="form-border">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(m=>m.Name)
            </th>
            <th>
                @Html.DisplayNameFor(m=>m.CategoryPlus.Name)
            </th>
            <th></th>
            <th></th>
        </tr>
        @foreach(var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(m => item.Name)
            </td>
            <td>
                @Html.DisplayFor(m => item.CategoryPlus.Name)
            </td>
            <td>
                @Html.Partial("_TableButtonPartial",item.Id)
            </td>
        </tr>

        }
    </table>
</div>
